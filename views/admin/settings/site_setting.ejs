<section class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-md-6">
                <h1 class="mb-2">Site Settings</h1>
                <ol class="breadcrumb float-sm-left">
                    <li class="breadcrumb-item"><a href="/dashboard">Home</a></li>
                    <li class="breadcrumb-item active">Settings - Site Settings</li>
                </ol>
            </div>
        </div>
        <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
</section>

<section class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-12">
                <div class="card">
                    <div class="card-body">
                        <form action="/dashboard/setting/term&condition" method="post" class="form"
                            enctype="multipart/form-data">
                            <div class="inputForm">
                                <label for="TextINput1">Logo</label>
                                <input name="logo" id="TextINput" class="input" type="file">
                            </div>
                            <div class="inputForm">
                                <label for="TextINput2">Company Name</label>
                                <input placeholder="Enter Job Tag" name="companyname" id="TextINput" class="input"
                                    type="text">
                            </div>
                            <div class="inputForm">
                                <label for="TextINput3">Email</label>
                                <input placeholder="Enter Job Tag" name="companyemail" id="TextINput" class="input"
                                    type="text">
                            </div>
                            <button type="submit" class="btn btn-dark">Submit</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>